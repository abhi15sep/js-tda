{
  "name": "JS-TDA",
  "description": "A browser-based JavaScript rewrite of Java's Thread Dump Analyzer",
  "main": [
    "dist/release/index.html",
    "dist/release/js-tda.min.css",
    "dist/release/js-tda.min.js"
  ],
  "dependencies": {
    "bootstrap": "~3.3.6",
    "jsrender": "~0.9.77",
    "datatables.net": "~1.10.12",
    "datatables.net-bs": "~1.10.12"
  },
  "moduleType": [
    "globals"
  ],
  "keywords": [
    "java",
    "thread",
    "analyzer"
  ],
  "authors": [
    "Greg Feigenson <kog@epiphanic.org>"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/kog/js-tda"
  },
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "build"
  ],
  "overrides": {
    "bootstrap": {
      "main": [
        "less/bootstrap.less",
        "dist/css/bootstrap.css",
        "dist/js/bootstrap.js",
        "fonts/glyphicons-halflings-regular.eot",
        "fonts/glyphicons-halflings-regular.svg",
        "fonts/glyphicons-halflings-regular.ttf",
        "fonts/glyphicons-halflings-regular.woff",
        "fonts/glyphicons-halflings-regular.woff2"
      ]
    }
  },
  "private": true
}